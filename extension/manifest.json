{
    "manifest_version":2,
    "version":"1.0",
    "name":"HACK.COMS Anti-Doomscroller",
    "icons": {
        "48": "/img/icon.svg",
        "96": "/img/icon.svg"
    },
    "browser_action":{
        "default_icon":{
            "19":"/img/icon.svg",
            "38":"/img/icon.svg"
        }
    },
    
    "permissions": [
        "<all_urls>",
        "browsingData",
        "contextualIdentities",
        "cookies",
        "management",
        "storage",
        "tabs",
        "webRequestBlocking",
        "webRequest"
    ],
    
    "content_scripts":[
        {
        "matches":["<all_urls>"],
        "js":["main.js"]
        }
    ]
  }